expectation Parent {
	task("one to a hundred million", $where = *) {
		task("subtask", $where);
	}
	task("has an event", $where) {
		notice("the event", $where);
	}
}

expectation Child {
	task("child's hundred million", *);
}

expectation Both {
	repeat between 1 and 2 {
		task(*, *) {
			xor {
				branch:
					task(*, *);
				branch:
					notice(*, *);
				branch:
			}
		}
	}
}
